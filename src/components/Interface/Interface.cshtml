  <div id="extras" class="ui-wrapper full-hw">
    <div class="button fl-c">
      <img src="src/assets/vector/files/ui/help.svg" alt="ui icon"/>
    </div>
  </div>
  -->
  <div id="toolbar" class="ui-wrapper full-w">
    <div @click="selectTool('cursor')" class="button fl-c" :class="ui.activeTool === 'cursor' && 'active'">
      <img src="src/assets/vector/files/ui/cursor.svg" alt="ui icon"/>
    </div>
    <div @click="openNodeMenu" class="button fl-c">
      <img src="src/assets/vector/files/ui/add.svg" alt="ui icon"/>
    </div>
    <div @click="selectTool('line')" class="button fl-c" :class="ui.activeTool === 'line' && 'active'">
      <img src="src/assets/vector/files/ui/line.svg" alt="ui icon"/>
    </div>
    <div class="button fl-c">
      <img src="src/assets/vector/files/ui/text.svg" alt="ui icon"/>
