<section id="mmui">
  <div id="topbar" class="ui-wrapper full-h">
    <div class="button fl-c">
      <img src="src/assets/vector/files/ui/save.svg" alt="ui icon"/>
    </div>
        <div class="button fl-c">
      <img src="src/assets/vector/files/ui/open.svg" alt="ui icon"/>
    </div>
    <div class="button fl-c">
      <img src="src/assets/vector/files/ui/undo.svg" alt="ui icon"/>
    </div>
    <div class="button fl-c">
      <img src="src/assets/vector/files/ui/redo.svg" alt="ui icon"/>
    </div>
  </div>
  <div id="toprightbar" class="full-h">
    <div class="ui-wrapper m-s-r">
      <div class="button fl-c">
        <img src="src/assets/vector/files/ui/settings.svg" alt="ui icon"/>
